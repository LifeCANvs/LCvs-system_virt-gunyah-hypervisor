interface mutex

// Mutex events.
//
// Note that there are separate events for before and after the various mutex
// operations.

event mutex_init
	param lock: mutex_t *

event mutex_acquire
	param lock: mutex_t *

event mutex_acquired
	param lock: mutex_t *

event mutex_failed
	param lock: mutex_t *

event mutex_release
	param lock: mutex_t *

event mutex_released
	param lock: mutex_t *
