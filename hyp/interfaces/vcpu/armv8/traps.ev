	return: vcpu_trap_result_t = VCPU_TRAP_RESULT_UNHANDLED

handled_event vcpu_trap_software_step_guest
	param esr: ESR_EL2_t
	return: vcpu_trap_result_t = VCPU_TRAP_RESULT_UNHANDLED

handled_event vcpu_trap_watchpoint_guest
